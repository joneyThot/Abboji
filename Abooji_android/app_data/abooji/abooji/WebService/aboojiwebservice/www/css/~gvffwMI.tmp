.input[type='range'] {

    -webkit-appearance: none;

    border-radius: 5px;

    background-color: #dcdcdc;

    margin: 0;

    vertical-align: middle;

    width: 100%;

}

.input[type='range']::-moz-range-track {

    -moz-appearance: none;

    border-radius: 5px;

    background-color: #fb9d54;

    height: 8px;

}

.input[type='range']::-moz-range-thumb {

    -moz-appearance: none;

    border-radius: 20px;

    background-color: #ebebeb;

    box-shadow: inset 0 0 10px rgba(000, 000, 000, 0.2);

    height: 15px;

    width: 15px;

}

.custom-dropdown__select {

    font-size: inherit;

    /* inherit size from .custom-dropdown */

    padding: .3em;

    /* add some space*/

    margin: 0;

    /* remove default margins */

}

.custom-dropdown__select--white {

    background: none repeat scroll 0 0 #F58220;

    border: medium none;

    border-radius: 4px;

    box-shadow: 0 3px 0 #DC6C0C, 0 -1px #DC6C0C inset;

    color: #FFFFFF;

    font-family: 'MyriadPro-Regular_1';

    font-size: 15px;

    outline: medium none;

    width: 100%;

}

.custom-dropdown {

    position: relative;

    display: inline-block;

    vertical-align: middle;

    width: 100%;

}

.custom-dropdown__select {

    /* accommodate with the pseudo elements for the dropdown arrow */

    border: 0;

    border-radius: 3px;

    -webkit-appearance: none;

    -moz-appearance: none;

    appearance: none;

}

.custom-dropdown::before,

.custom-dropdown::after {

    content: "";

    position: absolute;

    pointer-events: none;

}

.custom-dropdown::after {

    /*  Custom dropdown arrow */

    content: "\25BC";

    height: 1em;

    font-size: 22px;

    line-height: 1;

    right: .2em;

    top: 50%;

    margin-top: -.4em;

}

.custom-dropdown::before {

    /*  Custom dropdown arrow cover */

    width: 2em;

    right: 0;

    top: 0;

    bottom: 0;

    border-radius: 0 3px 3px 0;

}

.custom-dropdown__select[disabled] {

    color: #fff;

}

.custom-dropdown.custom-dropdown--disabled::after {

    color: #fff;

}

/* White dropdown style */

.custom-dropdown--white::before {

    bottom: .1em;

    background-color: #f58220;

    border-left: 1px solid #dc6c0c;

}

.custom-dropdown--white::after {

    color: #fff;

}

/* FF only temp fix */

.custom-dropdown--large .custom-dropdown__select {

    padding-right: 1.3em

}

.custom-dropdown--small .custom-dropdown__select {

    padding-right: .5em

}

